snippet sec "Comment Section"
" ===
" === $1
" ===
$2
endsnippet

snippet lua "lua << EOF"
lua << EOF
$1
EOF
endsnippet

snippet no "nnoremap"
noremap $1
endsnippet

snippet no "nnoremap"
nnoremap $1
endsnippet

snippet spa "SPACE"
<SPACE>$1
endsnippet

snippet sil "silent"
<silent> $1
endsnippet

snippet plu "Plug"
Plug '${1:plugname}'
endsnippet

snippet cr "CR"
<CR>$0
endsnippet
